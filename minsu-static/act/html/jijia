<script type="text/javascript">
	var myScroll1;
	var myScroll2;
	var myScroll3;
	var myScroll4;
	function loaded () {
		myScroll1 = new IScroll('.content_dizhi', { mouseWheel: true,preventDefault: false});
		myScroll2 = new IScroll('.content_huxing', { mouseWheel: true,preventDefault: false});
		myScroll3 = new IScroll('.content_gongneng', { mouseWheel: true,preventDefault: false});
		myScroll4 = new IScroll('.content_price', { mouseWheel: true,preventDefault: false});
	}
	document.addEventListener('touchmove', function (e) { e.preventDefault(); }, false);
        
        var action = "";
</script>


